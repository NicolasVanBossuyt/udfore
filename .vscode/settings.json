{
    "files.associations": {
        "buffer.h": "c",
        "lexer.h": "c",
        "token.h": "c"
    },
    "C_Cpp.default.cppStandard": "c++17",
    "C_Cpp.default.cStandard": "c11",
    "C_Cpp.default.intelliSenseMode": "gcc-x64",
    "C_Cpp.default.compilerArgs": [
        "-Wall",
        "-Wextra",
        "-Werror"
    ],
    "files.exclude": {
        "**/*.d": true,
        "**/*.o": true,
        "**/*.out": true
    }
}
